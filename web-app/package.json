{
  "name": "web-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:services": "jest --testPathPattern=services",
    "test:components": "jest --testPathPattern=components",
    "test:utils": "jest --testPathPattern=utils",
    "test:coverage-threshold": "jest --coverage --coverageThreshold='{}'",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:clear-cache": "jest --clearCache",
    "test:a11y": "jest --testPathPattern=accessibility",
    "monitor:performance": "node scripts/performance-check.js",
    "monitor:errors": "node scripts/error-check.js",
    "monitor:accessibility": "node scripts/accessibility-check.js",
    "audit:full": "npm run type-check && npm run lint && npm run test:coverage && npm run test:a11y",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.32.1",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.49.4",
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "file-saver": "^2.0.5",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.517.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-date-range": "^2.0.1",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^6.0.0",
    "react-grid-layout": "^1.5.1",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.5.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "babel-jest": "^30.0.2",
    "dotenv": "^16.5.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.2",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.2",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.0.0",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "tsc --noEmit"
    ]
  }
}
