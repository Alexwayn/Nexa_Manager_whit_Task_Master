{
  "name": "web-app",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/prebuild.js",
    "build": "npm run prebuild && vite build",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0 --config ../.config/eslint/eslint.config.js",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix --config ../.config/eslint/eslint.config.js",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" --config ../.config/prettier/.prettierrc",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" --config ../.config/prettier/.prettierrc",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "jest --config jest.config.cjs",
    "test:ci": "jest --config jest.config.cjs --ci --coverage --runInBand",
    "test:voice": "jest --config ./src/__tests__/config/jest.voice.config.json",
    "test:voice:watch": "jest --config ./src/__tests__/config/jest.voice.config.json --watch",
    "test:voice:coverage": "jest --config ./src/__tests__/config/jest.voice.config.json --coverage",
    "test:voice:unit": "jest --config ./src/__tests__/config/jest.voice.config.json --testPathPatterns=unit",
    "test:voice:integration": "jest --config ./src/__tests__/config/jest.voice.config.json --testPathPatterns=integration",
    "test:voice:components": "jest --config ./src/__tests__/config/jest.voice.config.json --testPathPatterns=components",
    "test:voice:services": "jest --config ./src/__tests__/config/jest.voice.config.json --testPathPatterns=services",
    "test:voice:performance": "node ./src/__tests__/utils/voiceTestRunner.js --mode=performance",
    "test:voice:debug": "jest --config ./src/__tests__/config/jest.voice.config.json --detectOpenHandles --forceExit",
    "test:voice:ci": "jest --config ./src/__tests__/config/jest.voice.config.json --ci --coverage --watchAll=false",
    "test:architecture": "jest --config ../.config/jest/jest.config.cjs --testPathPatterns=architecture",
    "validate:feature-api": "node scripts/validate-feature-api.js",
    "validate:structure": "npm run test:architecture",
    "lint:architecture": "eslint . --ext ts,tsx,js,jsx --config ../.config/eslint/eslint.config.js --no-eslintrc",
    "validate:build": "node scripts/validate-build.js",
    "build:validate": "npm run validate:build",
    "test:coverage:auth": "jest --config ../.config/jest/jest.config.cjs --testPathPatterns=features/auth --coverage --collectCoverageFrom='src/features/auth/**/*.{js,jsx,ts,tsx}'",
    "test:coverage:analytics": "jest --config ../.config/jest/jest.config.cjs --testPathPatterns=features/analytics --coverage --collectCoverageFrom='src/features/analytics/**/*.{js,jsx,ts,tsx}'",
    "test:coverage:calendar": "jest --config ../.config/jest/jest.config.cjs --testPathPatterns=features/calendar --coverage --collectCoverageFrom='src/features/calendar/**/*.{js,jsx,ts,tsx}'",
    "test:coverage:clients": "jest --config ../.config/jest/jest.config.cjs --testPathPatterns=features/clients --coverage --collectCoverageFrom='src/features/clients/**/*.{js,jsx,ts,tsx}'",
    "test:coverage:dashboard": "jest --config ../.config/jest/jest.config.cjs --testPathPatterns=features/dashboard --coverage --collectCoverageFrom='src/features/dashboard/**/*.{js,jsx,ts,tsx}'",
    "test:coverage:documents": "jest --config ../.config/jest/jest.config.cjs --testPathPatterns=features/documents --coverage --collectCoverageFrom='src/features/documents/**/*.{js,jsx,ts,tsx}'",
    "test:coverage:email": "jest --config ../.config/jest/jest.config.cjs --testPathPatterns=features/email --coverage --collectCoverageFrom='src/features/email/**/*.{js,jsx,ts,tsx}'",
    "test:coverage:financial": "jest --config ../.config/jest/jest.config.cjs --testPathPatterns=features/financial --coverage --collectCoverageFrom='src/features/financial/**/*.{js,jsx,ts,tsx}'",
    "test:coverage:scanner": "jest --config ../.config/jest/jest.config.cjs --testPathPatterns=features/scanner --coverage --collectCoverageFrom='src/features/scanner/**/*.{js,jsx,ts,tsx}'",
    "test:coverage:shared": "jest --config ../.config/jest/jest.config.cjs --testPathPatterns=shared --coverage --collectCoverageFrom='src/shared/**/*.{js,jsx,ts,tsx}'",
    "test:reports": "node scripts/test-reports.js",
    "test:reports:unit": "node scripts/test-reports.js unit",
    "test:reports:integration": "node scripts/test-reports.js integration",
    "test:reports:e2e": "node scripts/test-reports.js e2e",
    "test:reports:performance": "node scripts/test-reports.js performance",
    "test:reports:accessibility": "node scripts/test-reports.js accessibility",
    "test:reports:all": "node scripts/test-reports.js unit integration e2e performance accessibility",
    "playwright:install": "npx playwright install",
    "playwright:test": "npx playwright test --config ../.config/playwright/playwright.config.js",
    "playwright:test:headed": "npx playwright test --config ../.config/playwright/playwright.config.js --headed",
    "playwright:test:debug": "npx playwright test --config ../.config/playwright/playwright.config.js --debug",
    "monitor:performance": "node scripts/performance-check.js",
    "monitor:errors": "node scripts/error-check.js",
    "monitor:accessibility": "node scripts/accessibility-check.js",
    "audit:full": "npm run type-check && npm run lint && npm run test:coverage && npm run test:a11y",
    "audit:reports": "npm run test:reports:all && npm run monitor:performance && npm run monitor:accessibility",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.32.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@sentry/react": "^9.44.2",
    "@sentry/tracing": "^7.120.3",
    "@sentry/vite-plugin": "^3.5.0",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.82.0",
    "@tanstack/react-query-devtools": "^5.82.0",
    "@testing-library/user-event": "^14.6.1",
    "chart.js": "^4.4.9",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.20.1",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "json-2-csv": "^5.5.9",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.517.0",
    "lz-string": "^1.5.0",
    "pdfjs-dist": "^5.1.91",
    "qrcode": "^1.5.4",
    "quill": "^2.0.2",
    "react": "18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-date-range": "^2.0.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^4.0.0",
    "react-grid-layout": "^1.5.1",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.5.3",
    "react-icons": "^5.5.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.28.0",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^2.12.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "vite-plugin-environment": "^1.1.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@faker-js/faker": "^9.9.0",

    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.6.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^9.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "jest-junit": "^16.0.0",
    "jsdom": "^22.1.0",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.2",
    "vite": "^4.4.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --config ../.config/eslint/eslint.config.js",
      "prettier --write --config ../.config/prettier/.prettierrc",
      "npm run test:architecture --passWithNoTests"
    ],
    "*.{json,css,md}": [
      "prettier --write --config ../.config/prettier/.prettierrc"
    ],
    "*.{ts,tsx}": [
      "tsc --noEmit"
    ],
    "src/features/*/index.ts": [
      "npm run validate:feature-api --passWithNoTests"
    ]
  },
  "overrides": {
    "@testing-library/dom": "^10.4.1"
  }
}
