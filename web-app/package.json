{
  "name": "web-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0 --config ../.config/eslint/eslint.config.js",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix --config ../.config/eslint/eslint.config.js",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" --config ../.config/prettier/.prettierrc",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" --config ../.config/prettier/.prettierrc",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "jest --config ../.config/jest/jest.config.cjs",
    "test:watch": "jest --config ../.config/jest/jest.config.cjs --watch",
    "test:coverage": "jest --config ../.config/jest/jest.config.cjs --coverage",
    "test:ci": "jest --config ../.config/jest/jest.config.cjs --ci --coverage --watchAll=false",
    "test:debug": "jest --config ../.config/jest/jest.config.cjs --detectOpenHandles --forceExit",
    "test:unit": "jest --config ../.config/jest/jest.config.cjs --testPathPattern=unit",
    "test:integration": "jest --config ../.config/jest/jest.config.cjs --testPathPattern=integration",
    "test:e2e": "jest --config ../.config/jest/jest.config.cjs --testPathPattern=e2e",
    "test:services": "jest --config ../.config/jest/jest.config.cjs --testPathPattern=services",
    "test:components": "jest --config ../.config/jest/jest.config.cjs --testPathPattern=components",
    "test:utils": "jest --config ../.config/jest/jest.config.cjs --testPathPattern=utils",
    "test:coverage-threshold": "jest --config ../.config/jest/jest.config.cjs --coverage --coverageThreshold='{}'",
    "test:update-snapshots": "jest --config ../.config/jest/jest.config.cjs --updateSnapshot",
    "test:clear-cache": "jest --config ../.config/jest/jest.config.cjs --clearCache",
    "test:a11y": "jest --config ../.config/jest/jest.config.cjs --testPathPattern=accessibility",
    "test:reports": "node scripts/test-reports.js",
    "test:reports:unit": "node scripts/test-reports.js unit",
    "test:reports:integration": "node scripts/test-reports.js integration",
    "test:reports:e2e": "node scripts/test-reports.js e2e",
    "test:reports:performance": "node scripts/test-reports.js performance",
    "test:reports:accessibility": "node scripts/test-reports.js accessibility",
    "test:reports:all": "node scripts/test-reports.js unit integration e2e performance accessibility",
    "playwright:install": "npx playwright install",
    "playwright:test": "npx playwright test --config ../.config/playwright/playwright.config.js",
    "playwright:test:headed": "npx playwright test --config ../.config/playwright/playwright.config.js --headed",
    "playwright:test:debug": "npx playwright test --config ../.config/playwright/playwright.config.js --debug",
    "monitor:performance": "node scripts/performance-check.js",
    "monitor:errors": "node scripts/error-check.js",
    "monitor:accessibility": "node scripts/accessibility-check.js",
    "audit:full": "npm run type-check && npm run lint && npm run test:coverage && npm run test:a11y",
    "audit:reports": "npm run test:reports:all && npm run monitor:performance && npm run monitor:accessibility",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.32.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@sentry/react": "^9.31.0",
    "@sentry/tracing": "^7.120.3",
    "@sentry/vite-plugin": "^3.5.0",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.82.0",
    "@tanstack/react-query-devtools": "^5.82.0",
    "chart.js": "^4.4.9",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.20.1",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "json-2-csv": "^5.5.9",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.517.0",
    "lz-string": "^1.5.0",
    "pdfjs-dist": "^5.1.91",
    "qrcode": "^1.5.4",
    "quill": "^2.0.2",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-date-range": "^2.0.1",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^6.0.0",
    "react-grid-layout": "^1.5.1",
    "react-hook-form": "^7.58.1",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.5.3",
    "react-icons": "^5.5.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.6.3",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.22.0",
    "@playwright/test": "^1.54.1",
    "@stagewise/toolbar": "^0.5.0",
    "@stagewise/toolbar-react": "^0.5.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "babel-jest": "^30.0.2",
    "dotenv": "^16.5.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "express": "^5.1.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.2",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.2",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.0.0",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "tsc --noEmit"
    ]
  },
  "resolutions": {
    "glob": "^10.0.0",
    "inflight": "^1.0.6"
  }
}
